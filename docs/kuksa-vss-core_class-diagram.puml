@startuml
'https://plantuml.com/class-diagram

!startsub VssCore
package VssCore {
    VssNode <|- VssLeaf

    annotation VssModelGenerator

    interface VssNode {
        + children: Set<VssNode>
        + parentClass: KClass<*>
        + uuid: String
        + vssPath: String
        + description: String
        + type: String
        + comment: String
    }

    interface VssLeaf<T : Any> {
        + value: T
    }
}

!endsub

@enduml
